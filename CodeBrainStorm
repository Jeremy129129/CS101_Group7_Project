def dna_count(dna):
    dna = dna.upper()
    count_A = dna.count('A')
    count_C = dna.count('C')
    count_G = dna.count('G')
    count_T = dna.count('T')
    return [count_A,count_C,count_G,count_T]
    
def dna2rna(dna):
    rna = ''
    for symbol in dna:
        if symbol == 'A':
            rna = rna + 'U'
        elif symbol == 'T':
            rna = rna + 'A'
        elif symbol == 'C':
            rna = rna + 'G'
        elif symbol == 'G':
            rna = rna + 'C'
    return rna

def reverse_complement(dna):
    dna_rev = dna.reverse()
    rev_comp = '' 
    for symbol in dna_rev:
        if symbol == 'A':
            rev_comp = rev_comp + 'T'
        elif symbol == 'T':
            rev_comp = rev_comp + 'A'
        elif symbol == 'C':
            rev_comp = rev_comp + 'G'
        elif symbol == 'G':
            rev_comp = rev_comp + 'C'
    return rev_comp
